{
  "version": 2,
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/public/styles.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
} 